#: ../maps/brothers_lair_wind_hall.txt:224
msgid "Falling Wind Switch"
msgstr "Перемикач Падаючого Вітру"

#: ../maps/brothers_lair_wind_hall.txt:233
msgid "Falling Fire Switch"
msgstr "Перемикач Падаючого Вогню"

#: ../maps/brothers_lair_wind_hall.txt:245
